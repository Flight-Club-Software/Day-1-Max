#include <stdio.h>
 
int main(void)
{
    // number of rows
    int n = 2;
    int i, j;
 
    for (i = 1; i <= n; i++) 
    {
        // print space
        for (j = i; j < n; j++)
            printf("( ");
                // print space
        for (j = i; j < n; j++)
            printf(" )\n");
    }
 
    return 0;
}
